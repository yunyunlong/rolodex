{
    "name": "openpeer-rolodex",
    "version": "0.1.9",
    "pm": "npm",
    "publish": true,
    "main": "lib/rolodex.js",
    "dependencies": {
        "waitfor": "~0.1.3",
        "deepmerge": "~0.2.5",
        "escape-regexp-component": "~1.0.2",
        "passport": "~0.1.16",
        "redis": "~0.8.4",
        "hiredis": "~0.4.0",
        "request": "~2.20.0",
        "connect": "~2.7.8",
        "q": "~0.9.4",
        "cifre": "~0.0.8"
    },
    "scripts": {
        "postinstall": "cd example \n npm install --production"
    },
    "config": {
        "run": {
            "packagePath": "example"
        },
        "pio.deploy.converter": {
            "name": "nodejs-server",
            "main": "example/server.js"
        }
    },
    "overlay": {
        "scripts": {
            "postinstall": "cd openpeer \n npm install --production"
        },
        "config": {
            "run": {
                "packagePath": "openpeer"
            },
            "pio.deploy.converter": {
                "main": "openpeer/server.js"
            }
        }        
    }
}